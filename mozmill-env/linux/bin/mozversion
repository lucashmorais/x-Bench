#!/usr/bin/env python
# EASY-INSTALL-ENTRY-SCRIPT: 'mozversion==1.0','console_scripts','mozversion'
__requires__ = 'mozversion==1.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('mozversion==1.0', 'console_scripts', 'mozversion')()
    )
